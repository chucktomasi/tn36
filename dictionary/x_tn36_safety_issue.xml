<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Issue" max_length="40" name="x_tn36_safety_issue" type="collection">
        <element label="Description" mandatory="true" max_length="1000" name="description" type="string"/>
        <element label="Inspector" max_length="32" name="inspector" reference="sys_user" type="reference"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element label="Reported" mandatory="true" max_length="40" name="reported" type="glide_date_time"/>
        <element choice="3" default="New" label="State" mandatory="true" max_length="40" name="state" type="choice">
            <choice>
                <element label="New" sequence="0" value="New"/>
                <element label="Investigate" sequence="1" value="Investigate"/>
                <element label="Resolved" sequence="2" value="Resolved"/>
            </choice>
        </element>
        <element label="User" mandatory="true" max_length="32" name="user" reference="sys_user" type="reference"/>
        <index name="index">
            <element name="inspector"/>
        </index>
        <index name="index2">
            <element name="user"/>
        </index>
    </element>
</database>
